@article{Basser1994,
abstract = {The diagonal and off-diagonal elements of the effective self-diffusion tensor, Deff, are related to the echo intensity in an NMR spin-echo experiment. This relationship is used to design experiments from which Deff is estimated. This estimate is validated using isotropic and anisotropic media, i.e., water and skeletal muscle. It is shown that significant errors are made in diffusion NMR spectroscopy and imaging of anisotropic skeletal muscle when off-diagonal elements of Deff are ignored, most notably the loss of information needed to determine fiber orientation. Estimation of Deff provides the theoretical basis for a new MRI modality, diffusion tensor imaging, which provides information about tissue microstructure and its physiologic state not contained in scalar quantities such as T1, T2, proton density, or the scalar apparent diffusion constant.},
author = {Basser, Peter J. and Mattiello, J. and Lebihan, D.},
doi = {10.1006/jmrb.1994.1037},
issn = {10641866},
journal = {J. Magn. Reson. Ser. B},
month = {mar},
number = {3},
pages = {247--254},
title = {{Estimation of the Effective Self-Diffusion Tensor from the NMR Spin Echo}},
volume = {103},
year = {1994}
}
@incollection{Basser2009,
abstract = {This chapter reviews the basic principles governing the diffusion phenomenon and its measurement using magnetic resonance (MR). Diffusion is a mass transport process arising in nature, which results in molecular or particle mixing without requiring bulk motion and magnetic resonance provides a unique opportunity to quantify the diffusional characteristics of a wide range of specimens. Because diffusional processes are influenced by the geometrical structure of the environment, MR can be used to probe the structural environment non-invasively. This is particularly important in studies that involve biological samples in which the characteristic length of the boundaries influencing diffusion are typically so small that they cannot be resolved by conventional magnetic resonance imaging (MRI) techniques. As water undergoes diffusion, it also encounters barriers, macromolecules, and sampling many different local environments. Collectively, the signal measured in an MR experiment contains contributions from water motion in these various microenvironments. The challenge in diffusion NMR and MRI is to try to infer features of the local tissue anatomy, composition, and microstructure from MR displacement measurements. One great advantage of MR is that it permits one to probe tissue structure at different length scales—i.e. levels of hierarchical architectural organization. Thus, this imaging modality permits to study and elucidate complex structural features spanning length scales ranging from the macromolecular to the macroscopic—without the use of exogenous contrast agents.},
author = {Basser, Peter J. and {\"{O}}zarslan, Evren},
booktitle = {Diffus. MRI},
chapter = {1},
doi = {10.1016/B978-0-12-374709-9.00001-8},
editor = {Johansen-Berg, Heidi and Behrens, Timothy E. J.},
isbn = {9780123747099},
keywords = {DWI,Diffusion,MRI,anisotropy,apparent diffusion coefficient,diffusion tensor,magnetic resonance,propagator,q-space},
pages = {2--10},
publisher = {Academic Press, Inc.},
title = {{Introduction to Diffusion MR}},
year = {2009}
}
