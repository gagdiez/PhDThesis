file = {:user/ggallard/home/papers/boly{\_}resting{\_}state{\_}HBM2009.pdf:pdf},
isbn = {1097-0193 (Electronic)$\backslash$n1065-9471 (Linking)},
issn = {10659471},
journal = {Hum. Brain Mapp.},
keywords = {Consciousness,Default network,Functional MRI,Resting state,Vegetative state},
month = {aug},
