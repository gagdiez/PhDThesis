file = {:user/ggallard/home/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Barch et al. - 2013 - Function in the human connectome task-fMRI and individual differences in behavior(2).pdf:pdf},
issn = {10538119},
journal = {Neuroimage},
keywords = {Adult,Anatomic,Behavior,Behavior: physiology,Brain,Brain: anatomy {\&} histology,Brain: physiology,Connectome,Connectome: methods,Female,Humans,Magnetic Resonance Imaging,Magnetic Resonance Imaging: methods,Male,Models,Nerve Net,Nerve Net: anatomy {\&} histology,Nerve Net: physiology,Neurological,Task Performance and Analysis,Young Adult},
