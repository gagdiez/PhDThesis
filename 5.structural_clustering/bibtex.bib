journal = {Neuroimage},
keywords = {Algorithms,Anisotropy,Brain/*anatomy {\&} histology/*physiology,Computer-Assisted,Humans,Image Processing,Magnetic Resonance Imaging,Monte Carlo Method,Non-U.S. Gov't,Oxygen/blood,Research Support},
number = {1},
pages = {241--250},
pmid = {11969331},
title = {{An investigation of functional and anatomical connectivity using magnetic resonance imaging}},
volume = {16},
year = {2002}
}
