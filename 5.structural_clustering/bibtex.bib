eprint = {NIHMS150003},
file = {:user/ggallard/home/Downloads/2013{\_}-{\_}Glasser{\_}et{\_}al.{\_}-{\_}NeuroImage.pdf:pdf},
isbn = {1053-8119},
issn = {10538119},
journal = {Neuroimage},
keywords = {CIFTI,Grayordinates,Human Connectome Project,Image analysis pipeline,Multi-modal data integration,Surface-based analysis},
month = {oct},
